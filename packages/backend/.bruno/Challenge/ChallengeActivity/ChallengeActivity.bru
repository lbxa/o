meta {
  name: ChallengeActivity
  type: graphql
  seq: 1
}

post {
  url: {{URL}}/
  body: graphql
  auth: inherit
}

body:graphql {
  mutation {
    challengeActivityResultCreate(challengeActivityResultCreateInput: {
      activityId: "Q2hhbGxlbmdlQWN0aXZpdHk6MQ==",
      userId: "VXNlcjox",
      result: 1
    }) {
      user {
        firstName
        lastName
      }
      result
      createdAt
    }
  }
  
  
  
}
