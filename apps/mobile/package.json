{
  "name": "@o/mobile",
  "version": "1.0.4",
  "main": "entry.ts",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "expo start -c",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "eslint . --fix",
    "build": "yes | pnpm expo export --output-dir ./dist --platform all",
    "relay": "relay-compiler --watch",
    "typecheck": "tsc --noEmit",
    "expo:prod": "expo start -c --no-dev --minify",
    "prebuild": "expo prebuild --clean",
    "pod-install": "npx pod-install",
    "eas:build": "pnpx eas-cli@latest build",
    "eas:build:simulator": "pnpx eas-cli@latest build --profile development:simulator --platform ios",
    "eas:build:ios": "pnpx eas-cli@latest build --profile development --platform ios",
    "eas:device:create": "pnpx eas-cli@latest device:create",
    "eas:submit": "pnpx eas-cli@latest submit",
    "expo:doctor": "pnpx expo-doctor@latest",
    "expo:fix": "pnpx expo install --check"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "5.0.6",
    "@hookform/resolvers": "^3.9.0",
    "@o/api-gql": "workspace:*",
    "@o/eslint": "workspace:*",
    "@o/tailwind": "workspace:*",
    "@o/tsconfig": "workspace:*",
    "@react-native-community/datetimepicker": "8.2.0",
    "@react-native/assets-registry": "0.76.3",
    "@react-native/metro-config": "0.76.3",
    "@react-navigation/native": "7.0.0",
    "@rnx-kit/metro-config": "2.0.1",
    "@rnx-kit/metro-resolver-symlinks": "0.2.1",
    "babel-preset-expo": "12.0.2",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.13",
    "debounce": "^2.1.1",
    "expo": "52.0.11",
    "expo-asset": "11.0.1",
    "expo-build-properties": "0.13.1",
    "expo-checkbox": "4.0.0",
    "expo-constants": "17.0.3",
    "expo-dev-client": "5.0.4",
    "expo-haptics": "14.0.0",
    "expo-linking": "7.0.3",
    "expo-router": "4.0.9",
    "expo-secure-store": "14.0.0",
    "expo-splash-screen": "0.29.13",
    "expo-status-bar": "2.0.0",
    "immer": "10.1.1",
    "metro": "0.81.0",
    "metro-cache": "0.81.0",
    "metro-config": "0.81.0",
    "react": "18.3.1",
    "react-hook-form": "7.53.2",
    "react-native": "0.76.3",
    "react-native-gesture-handler": "2.20.2",
    "react-native-reanimated": "3.16.2",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "4.1.0",
    "react-native-svg": "15.8.0",
    "react-native-web": "~0.19.13",
    "react-relay": "^18.2.0",
    "react-relay-network-modern": "^6.2.2",
    "relay-runtime": "^18.2.0",
    "zod": "^3.23.8",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-transform-react-jsx": "7.25.9",
    "@babel/runtime": "7.24.6",
    "@types/react": "18.3.12",
    "@types/react-relay": "16.0.6",
    "@types/relay-runtime": "^18.1.1",
    "babel-plugin-relay": "18.0.0",
    "eslint": "8.57.1",
    "graphql": "^16.8.2",
    "nativewind": "4.1.23",
    "react-native-svg-transformer": "1.5.0",
