meta {
  name: HomeFeed
  type: graphql
  seq: 5
}

post {
  url: {{URL}}/graphql
  body: graphql
  auth: inherit
}

body:graphql {
  query { 
    viewer {
      homeFeed(first: 10) {
        edges {
          node {
            ... on UserRecord {
              user {
                firstName
              }
            }
            ... on StartingSoonChallenge {
              challenge {
                name
              }
              daysUntilStart
            }
            ... on EndingSoonChallenge {
              challenge {
                name
              }
              daysUntilEnd
            }
          }
        }
      }
    }
  }
}
